<Window x:Class="TerentievCourseWork.Views.ShellView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:TerentievCourseWork.Views"
      xmlns:viewModels="clr-namespace:TerentievCourseWork.ViewModels"
      mc:Ignorable="d"
      d:DesignHeight="980" d:DesignWidth="1920"
      d:DataContext="{d:DesignInstance Type=viewModels:ShellViewModel, IsDesignTimeCreatable=True}"
      WindowStartupLocation="CenterScreen"
      WindowState="Maximized"
      MinWidth="800" MinHeight="600"
      Title="{Binding AppTitle, Mode=OneWay}">
    <ContentControl x:Name="ActiveItem" />
</Window>
